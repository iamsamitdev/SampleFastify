{
  "name": "samplefastify",
  "version": "1.0.0",
  "main": "index.js",  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "./node_modules/.bin/tsc",
    "start": "node dist/index.js",
    "migrate": "node database/migrate.js",
    "generate-password": "node scripts/generate-password.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "vercel-build": "./node_modules/.bin/tsc",
    
    "docker:build": "docker build -t samplefastify:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.docker samplefastify:latest",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build -d",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all",
    "docker:logs": "docker-compose logs -f fastify_app",
    "docker:db": "docker-compose exec postgres psql -U postgres -d fastify_app"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/postgres": "^6.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.5.0",
    "fastify": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "typescript": "^5.8.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.11.6"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^24.0.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  }
}
